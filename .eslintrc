{
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "env": {
        "browser": true
    },
    "plugins": [
        "jquery"
    ],
    "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "module"
    },
    "rules": {
        "no-debugger": 0,
        "no-console": 0,
        "new-cap": 0,
        "strict": 0,
        "no-underscore-dangle": 0,
        "no-use-before-define": 0,
        "eol-last": 0,
        "quotes": [2, "single"]
    }
}